#!/bin/sh
# Startup of programs which are independent of window manager.

easyeffects --gapplication-service &
transmission-daemon &
udiskie &
case "$(hostnamectl hostname)" in
    *desktop*)
        [ -d "$HOME/data/.environments/flexget" ] && "$HOME/data/.environments/flexget/bin/flexget" daemon start -d &
        ;;
esac
